{
	"name": "picnic-planner-api",
	"version": "0.0.1",
	"description": "An application which helps in choosing a date and location, based on optimal weather conditions, for hosting picnics",
	"author": "WessCoby <wc@wesscoby.com>",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "dotenv -e .env.test -- jest --config ./test/unit/jest.unit.json --detectOpenHandles",
		"test:cov": "yarn test --coverage",
		"test:debug": "dotenv -e .env.test -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/unit/jest.unit.json --runInBand --detectOpenHandles",
		"test:e2e": "dotenv -e .env.test -- jest --config ./test/e2e/jest.e2e.json --runInBand --detectOpenHandles",
		"test:e2e:cov": "yarn test:e2e --coverage",
		"test:cache": "jest --clearCache",
		"test:full": "rimraf coverage && yarn test:e2e:cov && yarn test:cov"
	},
	"dependencies": {
		"@nestjs/axios": "^0.1.0",
		"@nestjs/common": "^9.0.0",
		"@nestjs/config": "^2.2.0",
		"@nestjs/core": "^9.0.0",
		"@nestjs/platform-express": "^9.0.0",
		"@nestjs/serve-static": "^3.0.0",
		"@nestjs/swagger": "^6.1.2",
		"@nestjs/terminus": "^9.1.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"helmet": "^6.0.0",
		"joi": "^17.6.4",
		"moment": "^2.29.4",
		"node-geocoder": "^4.2.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0",
		"swagger-ui-express": "^4.5.0",
		"tiny-invariant": "^1.3.1"
	},
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@golevelup/ts-jest": "^0.3.3",
		"@nestjs/cli": "^9.0.0",
		"@nestjs/schematics": "^9.0.0",
		"@nestjs/testing": "^9.0.0",
		"@types/express": "^4.17.13",
		"@types/jest": "28.1.8",
		"@types/node": "^16.0.0",
		"@types/node-geocoder": "^3.24.5",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"dotenv-cli": "^6.0.0",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-tsdoc": "^0.2.17",
		"fishery": "^2.2.2",
		"jest": "28.1.3",
		"jest-mock-extended": "^3.0.1",
		"prettier": "^2.3.2",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "28.0.8",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "4.1.0",
		"typescript": "^4.7.4"
	}
}
